        <activity android:name=".featurehighlight.TestFeatureHighlightActivity"/>
        <activity android:name=".coordinator.AppBarLayoutActivity"/>
        <activity android:name=".customview.SimpleViewGroupActivity"/>
        <activity android:name=".menu.TestMenuActivity"/>
        <activity android:name=".material.TestMaterialFeatureHighlightActivity"/>
        <activity android:name=".customview.XfermodeActivity"/>
        <activity android:name=".customview.ShaderActivity"/>
        <activity android:name=".customview.PathEffectActivity"/>
