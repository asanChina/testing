            <intent-filter android:label="I am handling">
                <action android:name="android.intent.action.SEND"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="plain/text"/>
            </intent-filter>
        </activity>
        <activity android:name=".drawable.TestDrawableActivity"/>
        <activity android:name=".materialdesign.TestMaterialDesignActivity"/>
        <activity android:name=".settings.SettingsActivity"/>
        <activity android:name=".progressbar.TestProgressBarActivity"/>

    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
